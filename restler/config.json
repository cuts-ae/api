  "AuthenticationToken": {
    "token_refresh_interval": 300,
    "token_refresh_command": "python3 get_token.py"
  },
  "EngineSettings": {
    "max_sequence_length": 100,
    "max_request_execution_time": 30,
    "time_budget": 3600,
    "checkers": {
      "useafterfree": {
        "mode": "exhaustive"
      },
      "leakagerule": {
        "mode": "exhaustive"
      },
      "resourcehierarchy": {
        "mode": "exhaustive"
      },
      "usebeforefree": {
        "mode": "exhaustive"
      },
      "namespacerule": {
        "mode": "exhaustive"
      },
      "invaliddynamicobject": {
        "mode": "exhaustive"
      },
      "payloadbody": {
        "mode": "exhaustive"
      },
      "examples": {
        "mode": "exhaustive"
      }
    },
    "no_ssl": true,
    "path_regex": null,
    "per_resource_settings": {
      "/api/v1/auth/login": {
        "producer_timing_delay": 1,
        "create_once": false
      },
      "/api/v1/auth/register": {
        "producer_timing_delay": 1,
        "create_once": false
      },
      "/api/v1/orders": {
        "producer_timing_delay": 2
      },
      "/api/v1/restaurants": {
        "producer_timing_delay": 2
      }
    },
    "global_producer_timing_delay": 1,
    "max_combinations": 20,
    "test_all_combinations": false,
    "include_user_agent": true,
    "user_agent": "RESTler/9.2.3 (Cuts.ae Security Testing)",
    "host": "localhost",
    "target_port": 45000,
    "request_throttle_ms": 100
  },
  "CheckerOptions": {
    "InvalidDynamicObjectChecker": {
      "InvalidValueChecker": {
        "strictSSRF": true
      }
    }
  },
  "FuzzingMode": "directed-smoke-test",
  "TargetIp": "localhost",
  "TargetPort": 45000,
  "MaxConcurrentRequests": 1,
  "ConnectionSettings": {
    "ProducerTimingDelay": 1,
    "RequestThrottleMs": 100
  }
}
